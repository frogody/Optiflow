{
  "name": "flow-orchestrator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port ${PORT:-3001}",
    "build": "NODE_OPTIONS='--max_old_space_size=8192' npx prisma generate && next build",
    "build:analyze": "ANALYZE=true npx prisma generate && next build",
    "postbuild": "echo 'Skipping bundle size check' || node scripts/check-bundle-size.js",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "voice-check": "ts-node scripts/validate-voice-routing.ts",
    "mcp:servers": "ts-node src/services/mcp/server/startServers.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run mcp:servers\"",
    "build:standalone": "next build",
    "docker:build": "docker build -t optiflow:latest .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "sh deploy.sh",
    "check-env": "ts-node scripts/check-env.ts || true",
    "generate-secret": "node scripts/generate-secret.js",
    "vercel-build": "NODE_OPTIONS='--max_old_space_size=8192' npx prisma generate && next build",
    "vercel-deploy": "vercel deploy --prod",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "prod:start": "NODE_ENV=production next start",
    "prod:build": "NODE_ENV=production next build",
    "prod:deploy": "next build && vercel deploy --prod",
    "db:seed": "ts-node prisma/seed.ts",
    "db:backup": "ts-node scripts/backup-db.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ci": "vitest run"
  },
  "dependencies": {
    "@11labs/client": "^0.1.3",
    "@anthropic-ai/sdk": "^0.24.0",
    "@auth/core": "^0.39.0",
    "@auth/prisma-adapter": "^2.8.0",
