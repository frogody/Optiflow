{
  "name": "flow-orchestrator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3000 next dev",
    "build": "npx prisma generate && next build",
    "start": "PORT=3000 next start",
    "lint": "next lint",
    "mcp:servers": "ts-node src/services/mcp/server/startServers.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run mcp:servers\"",
    "build:standalone": "next build",
    "docker:build": "docker build -t optiflow:latest .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "sh deploy.sh",
    "check-env": "ts-node scripts/check-env.ts",
    "generate-secret": "node scripts/generate-secret.js",
    "vercel-build": "SKIP_ENV_VALIDATION=1 NEXT_PUBLIC_NEXTAUTH_URL=$VERCEL_URL NODE_ENV=production npx prisma generate && next build",
    "vercel-deploy": "vercel deploy --prod",
    "postinstall": "npx prisma generate",
    "db:migrate": "ts-node scripts/migrate.ts",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "prod:start": "NODE_ENV=production next start",
    "prod:build": "NODE_ENV=production next build",
    "prod:deploy": "next build && vercel deploy --prod",
    "db:seed": "ts-node prisma/seed.ts",
    "db:backup": "ts-node scripts/backup-db.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@11labs/client": "^0.1.3",
    "@anthropic-ai/sdk": "^0.39.0",
    "@auth/core": "0.34.2",
    "@auth/prisma-adapter": "^2.8.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@floating-ui/react": "^0.27.7",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.0.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/font": "^13.5.3",
    "@pipedream/sdk": "^1.5.1",
    "@prisma/client": "^5.3.1",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.74.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "@types/three": "^0.160.0",
    "@vercel/analytics": "^1.1.1",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "bufferutil": "^4.0.9",
    "dotenv": "^16.5.0",
    "elevenlabs": "^1.57.0",
    "elevenlabs-node": "^2.0.3",
    "express": "^4.18.2",
    "framer-motion": "^10.18.0",
    "googleapis": "^148.0.0",
    "isomorphic-ws": "^5.0.0",
    "js-cookie": "^3.0.5",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "openai": "^4.95.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-speech-recognition": "^4.0.0",
    "reactflow": "^11.10.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "three": "^0.160.0",
    "utf-8-validate": "^6.0.5",
    "ws": "^8.18.1",
    "zod": "^3.22.2",
    "zustand": "^4.4.1",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "json-loader": "^0.5.7",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^24.0.0",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": "18.17.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
