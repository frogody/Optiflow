# Next.js Environment
NODE_ENV=development
NEXT_PUBLIC_API_URL=https://api.isyncso.com

# Authentication
AUTH_SECRET=your-auth-secret
SESSION_MAX_AGE=2592000 # 30 days in seconds

# Database Connection
