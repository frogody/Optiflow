{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install --no-frozen-lockfile",
  "framework": "nextjs",
  "github": {
    "enabled": false
  },
  "ignoreCommand": "echo 'Skipping integrity check'",
  "env": {
    "NEXT_PUBLIC_DEPLOYMENT_ENV": "production",
    "NEXT_PUBLIC_VERCEL_ENV": "production",
    "CI": "false"
  }
} 